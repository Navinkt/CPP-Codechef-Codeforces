#include<bits/stdc++.h>
using namespace std;

int main()
{int t;
  cin >> t;
  while (t--)
{long long int a,b=0;
cin>>a;
while(a!=0)
{
 b = b + (a%10);
 a = a / 10;
}
cout<<b<<endl;
}
return 0;
}


